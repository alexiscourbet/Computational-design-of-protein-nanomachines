      </OperateOnResidueSubset>
      <OperateOnResidueSubset name="design_surface_SASA" selector="pick_core_and_boundary_SASA">
        <PreventRepackingRLT/>
      </OperateOnResidueSubset>
      <OperateOnResidueSubset name="design_surface_SASA_end" selector="pick_core_and_boundary_SASA_end">
