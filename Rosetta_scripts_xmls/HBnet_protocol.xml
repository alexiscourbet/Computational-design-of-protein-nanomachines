      <And name="b_LIV" selectors="chain_b,LIV"/>
      <And name="AB_interface_LIV" selectors="AB_interface,LIV"/>
      <Not name="not_chain_a_LIV" selector="a_LIV"/>
      <Not name="not_chain_b_LIV" selector="b_LIV"/>
